// ==================== Sequence ====================

sequence( r A1  CUAAUACGAGAGGACCG )
//              ((((..((..))).)))

// ==================== Library ====================

//----- Fragment 1 -----
ncm_01 = library(
    pdb( "DB/2_2/CUCG/*.pdb.gz" )
    #1:#2, #3:#4 <- A1:A2, A16:A17
//  rmsd( 0.5 sidechain && !( pse || lp || hydrogen ) ) 
)
ncm_02 = library(
    pdb( "DB/2_2/UACC/*.pdb.gz" )
    #1:#2, #3:#4 <- A2:A3, A15:A16
//  rmsd( 0.5 sidechain && !( pse || lp || hydrogen ) ) 
)
ncm_03 = library(
    pdb( "DB/2_3/AAGAC/*.pdb.gz" )
    #1:#2, #3:#5 <- A3:A4, A13:A15
//  rmsd( 0.5 sidechain && !( pse || lp || hydrogen ) ) 
)
//----- Fragment 2 -----
lnk_01 = library(
    pdb( "DB/ss2/GG/*.pdb.gz" )
    #1:#2 <- A12:A13
//  rmsd( 0.5 sidechain && !( pse || lp || hydrogen ) ) 
)
ncm_04 = library(
    pdb( "DB/2_2/CGAG/*.pdb.gz" )
    #1:#2, #3:#4 <- A7:A8, A11:A12
//  rmsd( 0.5 sidechain && !( pse || lp || hydrogen ) ) 
)
ncm_05 = library(
    pdb( "DB/4/GAGA/*.pdb.gz" )
    #1:#4 <- A8:A11
//  rmsd( 0.5 sidechain && !( pse || lp || hydrogen ) ) 
)
lnk_02 = library(
    pdb( "DB/ss2/AU/*.pdb.gz" )
    #1:#2 <- A4:A5
//  rmsd( 0.5 sidechain && !( pse || lp || hydrogen ) ) 
)
lnk_03 = library(
    pdb( "DB/ss2/UA/*.pdb.gz" )
    #1:#2 <- A5:A6
//  rmsd( 0.5 sidechain && !( pse || lp || hydrogen ) ) 
)
// ===================== Backtrack =====================
structure = backtrack
(
    ncm_01
    merge( ncm_02  1.5 )
    merge( ncm_03  1.5 )
    merge( lnk_01  1.5 )
    merge( ncm_04  1.5 )
    merge( ncm_05  1.5 )
    merge( lnk_02  1.5 )
    merge( lnk_03  1.5 )
    //------ dangling ends --------
)

// =================== Distance Restraints ===================
distance(  A4:C1'    A5:C1'  0.0  10.2  )
distance(  A4:C1'    A6:C1'  0.0  14.6  )
distance(  A5:C1'    A7:C1'  0.0  14.6  )
distance(  A6:C1'    A7:C1'  0.0  10.2  )
// =================== Backtrack Restraints ===================
clash
(
    structure
    1.5 !( pse || lp || hydrogen )
)
backtrack_rst
(
    structure
    width_limit  = 25%,
    height_limit = 33%,
    method       = probabilistic
)
// =================== Ribose Restraints ===================
ribose_rst
(
    structure
    method    = ccm,
    pucker    = C3p_endo,
    threshold = 2.0
)
// =================== Exploration Initialization =========
explore
(
    structure
    option(
    model_limit = 1000,
    time_limit  = 30m,
    seed        = 3210 )
    rmsd( 3.0 sidechain && !( pse || lp || hydrogen ) )
    pdb( "struct_000010885" zipped )
)
