// ===================== Backtrack =====================
structure = backtrack
(
    ncm_01
    merge( lnk_01  1.5 )
    merge( lnk_02  1.5 )
    merge( lnk_03  1.5 )
    merge( lnk_04  1.5 )
    merge( lnk_05  1.5 )
    merge( lnk_06  1.5 )
    merge( ncm_02  1.5 )
    merge( ncm_03  1.5 )
    //------ dangling ends --------
    (  A16  A17  )
)

// =================== Distance Restraints ===================
distance(  A2:C1'    A3:C1'  0.0  10.2  )
distance(  A2:C1'    A4:C1'  0.0  14.6  )
distance(  A2:C1'    A5:C1'  0.0  19.0  )
distance(  A2:C1'    A6:C1'  0.0  23.4  )
distance(  A3:C1'    A7:C1'  0.0  23.4  )
distance(  A4:C1'    A7:C1'  0.0  19.0  )
distance(  A5:C1'    A7:C1'  0.0  14.6  )
distance(  A6:C1'    A7:C1'  0.0  10.2  )
// =================== Backtrack Restraints ===================
clash
(
    structure
    1.5 !( pse || lp || hydrogen )
)
backtrack_rst
(
    structure
    width_limit  = 25%,
    height_limit = 33%,
    method       = probabilistic
)
// =================== Ribose Restraints ===================
ribose_rst
(
    structure
    method    = ccm,
    pucker    = C3p_endo,
    threshold = 2.0
)
// =================== Exploration Initialization =========
explore
(
    structure
    option(
    model_limit = 1000,
    time_limit  = 30m,
    seed        = 3210 )
    rmsd( 3.0 sidechain && !( pse || lp || hydrogen ) )
    pdb( "struct_000039192" zipped )
)
