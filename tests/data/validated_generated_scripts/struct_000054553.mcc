// ==================== Sequence ====================

sequence( r A1  CUACUACGAGAGGACCG )
//              (((((...))..)))..

// ==================== Relations ====================
relation
(
A15:  A17 { file( "helixA_RNA" ) stack } 1
)
// ==================== Library ====================

//----- Fragment 1 -----
ncm_01 = library(
    pdb( "DB/2_2/CUAC/*.pdb.gz" )
    #1:#2, #3:#4 <- A1:A2, A14:A15
//  rmsd( 0.5 sidechain && !( pse || lp || hydrogen ) ) 
)
ncm_02 = library(
    pdb( "DB/2_2/UAGA/*.pdb.gz" )
    #1:#2, #3:#4 <- A2:A3, A13:A14
//  rmsd( 0.5 sidechain && !( pse || lp || hydrogen ) ) 
)
lnk_01 = library(
    pdb( "DB/ss2/AC/*.pdb.gz" )
    #1:#2 <- A3:A4
//  rmsd( 0.5 sidechain && !( pse || lp || hydrogen ) ) 
)
//----- Fragment 3 -----
ncm_03 = library(
    pdb( "DB/2_2/CUAG/*.pdb.gz" )
    #1:#2, #3:#4 <- A4:A5, A9:A10
//  rmsd( 0.5 sidechain && !( pse || lp || hydrogen ) ) 
)
ncm_04 = library(
    pdb( "DB/5/UACGA/*.pdb.gz" )
    #1:#5 <- A5:A9
//  rmsd( 0.5 sidechain && !( pse || lp || hydrogen ) ) 
)
lnk_02 = library(
    pdb( "DB/ss2/GA/*.pdb.gz" )
    #1:#2 <- A10:A11
//  rmsd( 0.5 sidechain && !( pse || lp || hydrogen ) ) 
)
lnk_03 = library(
    pdb( "DB/ss2/AG/*.pdb.gz" )
    #1:#2 <- A11:A12
//  rmsd( 0.5 sidechain && !( pse || lp || hydrogen ) ) 
)
// ===================== Backtrack =====================
structure = backtrack
(
    ncm_01
    merge( ncm_02  1.5 )
    merge( lnk_01  1.5 )
    merge( ncm_03  1.5 )
    merge( ncm_04  1.5 )
    merge( lnk_02  1.5 )
    merge( lnk_03  1.5 )
    //------ dangling ends --------
    (  A15  A16  A17  )
)

// =================== Distance Restraints ===================
distance(  A10:C1'    A11:C1'  0.0  10.2  )
distance(  A10:C1'    A12:C1'  0.0  14.6  )
distance(  A11:C1'    A13:C1'  0.0  14.6  )
distance(  A12:C1'    A13:C1'  0.0  10.2  )
// =================== Backtrack Restraints ===================
clash
(
    structure
    1.5 !( pse || lp || hydrogen )
)
backtrack_rst
(
    structure
    width_limit  = 25%,
    height_limit = 33%,
    method       = probabilistic
)
// =================== Ribose Restraints ===================
ribose_rst
(
    structure
    method    = ccm,
    pucker    = C3p_endo,
    threshold = 2.0
)
// =================== Exploration Initialization =========
explore
(
    structure
    option(
    model_limit = 1000,
    time_limit  = 30m,
    seed        = 3210 )
    rmsd( 3.0 sidechain && !( pse || lp || hydrogen ) )
    pdb( "struct_000054553" zipped )
)
